{
  "name": "rescript-project-template",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "es6-global",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": ["-open RescriptCore"],
  "bs-dependencies": ["@rescript/core","@ocsigen/ts2ocaml"],
  "gentypeconfig": {
    "module": "es6",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.tsx",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
